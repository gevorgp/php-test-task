{
  "name": "htr3n/php-project-template",
  "description": "A basic PSR-4 project skeleton for PHP development",
  "keywords": [
    "php",
    "project",
    "psr4",
    "phpunit",
    "testing",
    "skeleton",
    "template"
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.20"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "src/App/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "Tests/App/"
    }
  },
  "minimum-stability": "stable"
}
